{
  "name": "portifolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "sh prepare.sh",
    "lint": "eslint --fix \"**/*.{ts,tsx}\"",
    "prettier": "prettier --write \"./**/*.{js,jsx,ts,tsx}\""
